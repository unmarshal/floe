-- Test runtime schema validation catches missing columns

schema Expected {
    id: String,
    name: String,
    email: String,
}

schema Output {
    id: String,
    name: String,
    email: String,
}

let passthrough : Expected -> Output =
    map {
        id: .id,
        name: .name,
        email: .email
    }

main =
    read "input.parquet" as Expected
    |> passthrough
    sink "output.parquet"
